
@model GravityWebExt.Models.MeasurementData
@{
    ViewData["Title"] = "Контрольная панель";
}


<title>Панель измерений</title>
<meta charset="utf-8">
<meta name="viewport" content="width=device-width, initial-scale=1">

<style type="text/css">

    

    #border {
        border: 2px solid #DEE2E6;
    }

        #border:hover {
